 {{#each this}}
    <div class="photo-card">
  <img class="card-image" src="{{this.webformatURL}}" alt="{{this.tags}}" loading="lazy" />
  <div class="info">
    <p class="info-item">
  <b class="info-item__title">Likes</b>
      {{this.likes}}
    </p>
    <p class="info-item">
      <b class="info-item__title">Views</b>
       {{this.views}}
    </p>
    <p class="info-item">
      <b class="info-item__title">Comments</b>
      {{this.comments}}
    </p>
    <p class="info-item">
      <b class="info-item__title">Downloads</b>
      {{this.downloads}}
    </p>
  </div>
</div>
  {{/each}}